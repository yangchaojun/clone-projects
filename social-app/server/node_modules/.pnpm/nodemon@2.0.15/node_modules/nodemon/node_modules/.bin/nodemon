#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/projects/clone/social-app/server/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/bin/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/nodemon@2.0.15/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules:/root/projects/clone/social-app/server/node_modules:/root/projects/clone/social-app/node_modules:/root/projects/clone/node_modules:/root/projects/node_modules:/root/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/projects/clone/social-app/server/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/bin/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/nodemon@2.0.15/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules:/root/projects/clone/social-app/server/node_modules:/root/projects/clone/social-app/node_modules:/root/projects/clone/node_modules:/root/projects/node_modules:/root/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
