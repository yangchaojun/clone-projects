#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/projects/clone/social-app/server/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/is-ci@2.0.0/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules:/root/projects/clone/social-app/server/node_modules:/root/projects/clone/social-app/node_modules:/root/projects/clone/node_modules:/root/projects/node_modules:/root/node_modules:/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules/is-ci/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/projects/clone/social-app/server/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/is-ci@2.0.0/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules:/root/projects/clone/social-app/server/node_modules:/root/projects/clone/social-app/node_modules:/root/projects/clone/node_modules:/root/projects/node_modules:/root/node_modules:/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules/is-ci/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../is-ci/bin.js" "$@"
else
  exec node  "$basedir/../is-ci/bin.js" "$@"
fi
