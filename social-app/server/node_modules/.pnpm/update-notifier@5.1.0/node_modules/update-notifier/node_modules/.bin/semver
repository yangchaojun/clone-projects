#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/projects/clone/social-app/server/node_modules/.pnpm/semver@7.3.5/node_modules/semver/bin/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/semver@7.3.5/node_modules/semver/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/semver@7.3.5/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules:/root/projects/clone/social-app/server/node_modules:/root/projects/clone/social-app/node_modules:/root/projects/clone/node_modules:/root/projects/node_modules:/root/node_modules:/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/update-notifier@5.1.0/node_modules/update-notifier/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/update-notifier@5.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/projects/clone/social-app/server/node_modules/.pnpm/semver@7.3.5/node_modules/semver/bin/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/semver@7.3.5/node_modules/semver/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/semver@7.3.5/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules:/root/projects/clone/social-app/server/node_modules:/root/projects/clone/social-app/node_modules:/root/projects/clone/node_modules:/root/projects/node_modules:/root/node_modules:/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/update-notifier@5.1.0/node_modules/update-notifier/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/update-notifier@5.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@7.3.5/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../semver@7.3.5/node_modules/semver/bin/semver.js" "$@"
fi
