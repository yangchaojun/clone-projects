#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/projects/clone/social-app/server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.8/node_modules/@mapbox/node-pre-gyp/bin/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.8/node_modules/@mapbox/node-pre-gyp/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.8/node_modules/@mapbox/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.8/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules:/root/projects/clone/social-app/server/node_modules:/root/projects/clone/social-app/node_modules:/root/projects/clone/node_modules:/root/projects/node_modules:/root/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/projects/clone/social-app/server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.8/node_modules/@mapbox/node-pre-gyp/bin/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.8/node_modules/@mapbox/node-pre-gyp/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.8/node_modules/@mapbox/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/@mapbox+node-pre-gyp@1.0.8/node_modules:/root/projects/clone/social-app/server/node_modules/.pnpm/node_modules:/root/projects/clone/social-app/server/node_modules:/root/projects/clone/social-app/node_modules:/root/projects/clone/node_modules:/root/projects/node_modules:/root/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/node-pre-gyp" "$@"
else
  exec node  "$basedir/../../bin/node-pre-gyp" "$@"
fi
